today = new Date();
time1 = today.getHours() + ":" + today.getMinutes() + ":" + today.getSeconds();

var time3 = (today.getTime() / 1000).toFixed(0);
var time4 = (today1.getTime() / 1000).toFixed(0);

const Hours = Math.floor(((time4 - time3) / 3600) % 24);
const Minuts = Math.floor((((time4 - time3) / 60) % 60));
const seconds = ((time4 - time3) % 60);
const totalTime = Hours + ":" + Minuts + ":" + seconds;